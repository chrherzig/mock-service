<wsdl:definitions
    name="BlackListDeltaService"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
    xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
    xmlns:cm="http://tsystems.com/etccm/iso12855/common"
    xmlns:el="http://tsystems.com/etccm/iso12855/exceptionList"
    xmlns:tns="http://tsystems.com/etccm/iso12855/blackListDelta"
    targetNamespace="http://tsystems.com/etccm/iso12855/blackListDelta">

    <wsdl:documentation>
        Copyright (c) 2015 T-Systems International GmbH
        All Rights Reserved

        This software is the confidential and proprietary information of
        T-Systems International GmbH ("Confidential Information").
        You shall not disclose such Confidential Information and shall
        use it only in accordance with the terms of the license agreement
        you entered into with T-Systems International GmbH.
    </wsdl:documentation>

    <wsdl:types>
        <xsd:schema targetNamespace="http://tsystems.com/etccm/iso12855/blackListDelta">
            <xsd:import schemaLocation="Common.xsd" namespace="http://tsystems.com/etccm/iso12855/common" />
            <xsd:import schemaLocation="BlackListDelta.xsd" namespace="http://tsystems.com/etccm/iso12855/exceptionList" />
        </xsd:schema>
    </wsdl:types>

    <wsdl:message name="exceptionListRequest">
        <wsdl:part name="request" element="el:InfoExchangeExceptionList" />
    </wsdl:message>

    <wsdl:message name="exceptionListResponse">
        <wsdl:part name="response" element="cm:response" />
    </wsdl:message>

    <wsdl:portType name="BlackListDeltaServicePT">
        <wsdl:operation name="sendBlackListDelta">
            <wsdl:input message="tns:exceptionListRequest" />
            <wsdl:output message="tns:exceptionListResponse" />
        </wsdl:operation>
    </wsdl:portType>

    <wsdl:binding name="BlackListDeltaServiceBinding" type="tns:BlackListDeltaServicePT">
        <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http" />
        <wsdl:operation name="sendBlackListDelta">
            <soap:operation soapAction="http://tsystems.com/etccm/iso12855/BlackListDeltaService/sendBlackListDelta" />
            <wsdl:input>
                <soap:body use="literal" />
            </wsdl:input>
            <wsdl:output>
                <soap:body use="literal" />
            </wsdl:output>
        </wsdl:operation>
    </wsdl:binding>

    <wsdl:service name="BlackListDeltaService">
        <wsdl:port name="BlackListDeltaServicePort" binding="tns:BlackListDeltaServiceBinding">
            <soap:address location="http://localhost/BlackListDeltaService" />
        </wsdl:port>
    </wsdl:service>

</wsdl:definitions>
