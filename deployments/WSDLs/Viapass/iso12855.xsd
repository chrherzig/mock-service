         <xsd:element name="timeOfCCCRecord" minOccurs="0" type="DateAndTime"/>
         <xsd:element name="axlesHistory" minOccurs="0" type="VehicleAxlesHistory"/>
         <xsd:element name="commStatus" minOccurs="0" type="CommunicationStatus"/>
         <xsd:element name="gnssStatus" minOccurs="0" type="GnssStatus"/>
         <xsd:element name="distRecStatus" minOccurs="0" type="DistanceRecordingStatus"/>
         <xsd:element name="activeContext" minOccurs="0" type="ActiveContext"/>
         <xsd:element name="obeHistory" minOccurs="0" type="OBEStatusHistory"/>
      </xsd:sequence>
