         <xsd:element name="tollCharger" minOccurs="0" type="TollCharger"/>
         <xsd:element name="timeOfReport" minOccurs="0" type="DateAndTime"/>
         <xsd:element name="reportPeriod" type="Period"/>
         <xsd:element name="versionInfo" minOccurs="0" type="VersionID"/>
         <xsd:element name="usageStatementList" >
            <xsd:complexType>
               <xsd:sequence minOccurs="0" maxOccurs="unbounded">
                  <xsd:element name="UsageStatement" type="UsageStatement"/>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="vatForThisSession" minOccurs="0" type="PaymentFee"/>
         <xsd:element name="accountStatus" minOccurs="0" type="AccountStatus"/>
         <xsd:element name="transactionCounter" minOccurs="0" >
            <xsd:simpleType>
               <xsd:restriction base="xsd:unsignedInt"/>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="mileage" minOccurs="0" type="Distance"/>
         <xsd:element name="listOfCCCAttributes" minOccurs="0" >
            <xsd:complexType>
               <xsd:sequence minOccurs="0" maxOccurs="unbounded">
                  <xsd:element name="CCCAttributes" type="CCCAttributes"/>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="authenticator" minOccurs="0" type="MessageAuthenticatorChargingModule"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="Duration">
      <xsd:sequence>
         <xsd:element name="dur" type="REAL"/>
         <xsd:element name="durUnit" minOccurs="0" type="DurUnit"/>
