      <xsd:sequence>
         <xsd:element name="obeId" minOccurs="0" type="ObeId"/>
         <xsd:element name="vehicleLPNr" minOccurs="0" type="VehicleLicencePlateNumber"/>
         <xsd:element name="paymentMeans" minOccurs="0" type="PaymentMeans"/>
