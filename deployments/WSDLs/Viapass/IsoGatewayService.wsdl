<?xml version="1.0" encoding="UTF-8"?>

<wsdl:definitions name="IsoGatewayService" targetNamespace="http://tsystems.com/gw/ws/iso" xmlns:tns="http://tsystems.com/gw/ws/iso"
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
	xmlns:xmime="http://www.w3.org/2005/05/xmlmime">
	<wsdl:documentation>
